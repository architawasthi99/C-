{
    int age;
    cout<<"ENTER YOUR AGE: ";
    cin>>age;
    switch(age){
        case 18:
            cout<<"KID PASS";
            break;
        case 19:
        case 20:
        case 21:
        cout<<"ALLOWED";
        break;
    }
    if(age<18){
        cout<<"not allowed";
    }
    else{
        cout<<"allowed";
    }
return 0;
}
