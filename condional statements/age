#include<iostream>
using namespace std;
int main(){
    int age;
    cout<<"ENTER YOUR AGE: ";
    cin>>age;
    if(age<18){
        cout<<"YOU ARE NOT ALLOWED YOUR AGE"<<age;
    }
    else if (age==18){
        cout<<"YOU WILL GET KID PASS AGE="<<age;
    }
    else{
        cout<<"YOU ARE ALLOWED AGE="<<age;
    }
    return 0;
}
