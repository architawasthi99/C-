#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> vec={1,2,3,4,5,6,7,8}; 
    for (int i : vec) { 
        cout << i << " ";
    }
    cout<<endl<<"size of vector is "<<vec.size()<<endl;
    vec.push_back(25);
    cout<<"size of vector after push is "<<vec.size()<<endl;
    for (int i : vec) { 
        cout << i << " ";
    }
    
    vec.pop_back();
    vec.pop_back();
    vec.pop_back();
    cout<<endl<<"size of vector after pop is "<<vec.size()<<endl;
    for (int i : vec) { 
        cout << i << " ";
    }
    cout<<endl<<"first element of vector is "<<vec.front()<<endl;
    cout<<"last element of vector is "<<vec.back()<<endl;
    
    cout<<"element at index 0 "<<vec.at(0)<<endl;
    cout<<"element at index 2 "<<vec.at(2)<<endl;
    cout<<"element at index 1 "<<vec.at(1)<<endl;
    cout<<"element at index 3 "<<vec.at(3)<<endl;
    
    return 0;
}

